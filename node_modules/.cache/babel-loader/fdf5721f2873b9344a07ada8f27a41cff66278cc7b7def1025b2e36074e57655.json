","name","required","rows"],"sources":["C:/Users/Admin/Downloads/Interview/communion-hub/src/components/EventHeader.js"],"sourcesContent":["import React, { useState } from \"react\";\r\nimport { Navbar, Nav, Modal, Button, Container } from \"react-bootstrap\";\r\nimport logo from \"../assets/logo.png\"; // Adjust path according to your folder structure\r\n\r\nfunction EventHeader({ categories, onFilter, onAddEvent }) {\r\n    const [showModal, setShowModal] = useState(false);\r\n\r\n    // Toggle the Add Event modal\r\n    const handleToggleModal = () => setShowModal(!showModal);\r\n\r\n    return (\r\n        <Navbar bg=\"dark\" expand=\"lg\" variant=\"dark\" sticky=\"top\">\r\n            <Container>\r\n                {/* Brand Name with Logo */}\r\n                <Navbar.Brand href=\"/\" className=\"d-flex align-items-center\">\r\n                    <img\r\n                        src={logo}\r\n                        alt=\"CommunionHub Logo\"\r\n                        style={{ height: \"40px\", marginRight: \"10px\" }}\r\n                    />\r\n                    CommunionHub\r\n                </Navbar.Brand>\r\n\r\n                {/* Filter Dropdown */}\r\n                <Nav className=\"ms-auto d-flex align-items-center\">\r\n                    <select\r\n                        id=\"categoryFilter\"\r\n                        className=\"form-select\"\r\n                        onChange={(e) => onFilter(e.target.value)}\r\n                        style={{ width: \"200px\" }}\r\n                    >\r\n                        <option value=\"\">All Categories</option>\r\n                        {categories.map((category, index) => (\r\n                            <option key={index} value={category}>\r\n                                {category}\r\n                            </option>\r\n                        ))}\r\n                    </select>\r\n                </Nav>\r\n\r\n                {/* Toggle Icon (☰) for Popup */}\r\n                <Button\r\n                    variant=\"outline-light\"\r\n                    className=\"ms-3\"\r\n                    onClick={handleToggleModal}\r\n                >\r\n                    ☰\r\n                </Button>\r\n            </Container>\r\n\r\n            {/* Add Event Popup Modal */}\r\n            <Modal show={showModal} onHide={handleToggleModal} centered>\r\n                <Modal.Header closeButton>\r\n                    <Modal.Title>Add New Event</Modal.Title>\r\n                </Modal.Header>\r\n                <Modal.Body>\r\n                    <form\r\n                        onSubmit={(e) => {\r\n                            e.preventDefault();\r\n                            const newEvent = {\r\n                                title: e.target.title.value,\r\n                                date: e.target.date.value,\r\n                                location: e.target.location.value,\r\n                                description: e.target.description.value,\r\n                                category: e.target.category.value,\r\n                            };\r\n                            onAddEvent(newEvent);\r\n                            setShowModal(false);\r\n                        }}\r\n                    >\r\n                        <div className=\"mb-3\">\r\n                            <label className=\"form-label\">Title</label>\r\n                            <input\r\n                                type=\"text\"\r\n                                name=\"title\"\r\n                                className=\"form-control\"\r\n                                required\r\n                            />\r\n                        </div>\r\n                        <div className=\"mb-3\">\r\n                            <label className=\"form-label\">Date</label>\r\n                            <input\r\n                                type=\"date\"\r\n                                name=\"date\"\r\n                                className=\"form-control\"\r\n                                required\r\n                            />\r\n                        </div>\r\n                        <div className=\"mb-3\">\r\n                            <label className=\"form-label\">Location</label>\r\n                            <input\r\n                                type=\"text\"\r\n                                name=\"location\"\r\n                                className=\"form-control\"\r\n                                required\r\n                            />\r\n                        </div>\r\n                        <div className=\"mb-3\">\r\n                            <label className=\"form-label\">Description</label>\r\n                            <textarea\r\n                                name=\"description\"\r\n                                className=\"form-control\"\r\n                                rows=\"3\"\r\n                                required\r\n                            ></textarea>\r\n                        </div>\r\n                        <div className=\"mb-3\">\r\n                            <label className=\"form-label\">Category</label>\r\n                            <select name=\"category\" className=\"form-select\" required>\r\n                                <option value=\"\">Select Category</option>\r\n                                {categories.map((category, index) => (\r\n                                    <option key={index} value={category}>\r\n                                        {category}\r\n                                    </option>\r\n                                ))}\r\n                            </select>\r\n                        </div>\r\n                        <Button type=\"submit\" variant=\"primary\" className=\"w-100\">\r\n                            Submit\r\n                        </Button>\r\n                    </form>\r\n                </Modal.Body>\r\n            </Modal>\r\n        </Navbar>\r\n    );\r\n}\r\n\r\nexport default EventHeader;\r\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAIC,QAAQ,KAAQ,OAAO,CACvC,OAASC,MAAM,CAAEC,GAAG,CAAEC,KAAK,CAAEC,MAAM,CAAEC,SAAS,KAAQ,iBAAiB,CACvE,MAAO,CAAAC,IAAI,KAAM,oBAAoB,CAAE;AAAA,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAEvC,QAAS,CAAAC,WAAWA,CAAAC,IAAA,CAAuC,IAAtC,CAAEC,UAAU,CAAEC,QAAQ,CAAEC,UAAW,CAAC,CAAAH,IAAA,CACrD,KAAM,CAACI,SAAS,CAAEC,YAAY,CAAC,CAAGjB,QAAQ,CAAC,KAAK,CAAC,CAEjD;AACA,KAAM,CAAAkB,iBAAiB,CAAGA,CAAA,GAAMD,YAAY,CAAC,CAACD,SAAS,CAAC,CAExD,mBACIN,KAAA,CAACT,MAAM,EAACkB,EAAE,CAAC,MAAM,CAACC,MAAM,CAAC,IAAI,CAACC,OAAO,CAAC,MAAM,CAACC,MAAM,CAAC,KAAK,CAAAC,QAAA,eACrDb,KAAA,CAACL,SAAS,EAAAkB,QAAA,eAENb,KAAA,CAACT,MAAM,CAACuB,KAAK,EAACC,IAAI,CAAC,GAAG,CAACC,SAAS,CAAC,2BAA2B,CAAAH,QAAA,eACxDf,IAAA,QACImB,GAAG,CAAErB,IAAK,CACVsB,GAAG,CAAC,mBAAmB,CACvBC,KAAK,CAAE,CAAEC,MAAM,CAAE,MAAM,CAAEC,WAAW,CAAE,MAAO,CAAE,CAClD,CAAC,eAEN,EAAc,CAAC,cAGfvB,IAAA,CAACN,GAAG,EAACwB,SAAS,CAAC,mCAAmC,CAAAH,QAAA,cAC9Cb,KAAA,WACIsB,EAAE,CAAC,gBAAgB,CACnBN,SAAS,CAAC,aAAa,CACvBO,QAAQ,CAAGC,CAAC,EAAKpB,QAAQ,CAACoB,CAAC,CAACC,MAAM,CAACC,KAAK,CAAE,CAC1CP,KAAK,CAAE,CAAEQ,KAAK,CAAE,OAAQ,CAAE,CAAAd,QAAA,eAE1Bf,IAAA,WAAQ4B,KAAK,CAAC,EAAE,CAAAb,QAAA,CAAC,gBAAc,CAAQ,CAAC,CACvCV,UAAU,CAACyB,GAAG,CAAC,CAACC,QAAQ,CAAEC,KAAK,gBAC5BhC,IAAA,WAAoB4B,KAAK,CAAEG,QAAS,CAAAhB,QAAA,CAC/BgB,QAAQ,EADAC,KAEL,CACX,CAAC,EACE,CAAC,CACR,CAAC,cAGNhC,IAAA,CAACJ,MAAM,EACHiB,OAAO,CAAC,eAAe,CACvBK,SAAS,CAAC,MAAM,CAChBe,OAAO,CAAEvB,iBAAkB,CAAAK,QAAA,CAC9B,QAED,CAAQ,CAAC,EACF,CAAC,cAGZb,KAAA,CAACP,KAAK,EAACuC,IAAI,CAAE1B,SAAU,CAAC2B,MAAM,CAAEzB,iBAAkB,CAAC0B,QAAQ,MAAArB,QAAA,eACvDf,IAAA,CAACL,KAAK,CAAC0C,MAAM,EAACC,WAAW,MAAAvB,QAAA,cACrBf,IAAA,CAACL,KAAK,CAAC4C,KAAK,EAAAxB,QAAA,CAAC,eAAa,CAAa,CAAC,CAC9B,CAAC,cACff,IAAA,CAACL,KAAK,CAAC6C,IAAI,EAAAzB,QAAA,cACPb,KAAA,SACIuC,QAAQ,CAAGf,CAAC,EAAK,CACbA,CAAC,CAACgB,cAAc,CAAC,CAAC,CAClB,KAAM,CAAAC,QAAQ,CAAG,CACbC,KAAK,CAAElB,CAAC,CAACC,MAAM,CAACiB,KAAK,CAAChB,KAAK,CAC3BiB,IAAI,CAAEnB,CAAC,CAACC,MAAM,CAACkB,IAAI,CAACjB,KAA